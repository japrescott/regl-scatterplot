{
  "name": "regl-scatterplot",
  "version": "0.18.6",
  "description": "WebGL-based 2D Scatterplot in Regl",
  "author": "Fritz Lekschas",
  "keywords": [
    "scatterplot",
    "2d",
    "regl",
    "webgl"
  ],
  "license": "MIT",
  "main": "dist/regl-scatterplot.js",
  "module": "dist/regl-scatterplot.esm.js",
  "files": [
    "src/*",
    "dist/*"
  ],
  "scripts": {
    "build": "npm run build-library; npm run build-demo",
    "build-library": "rm -rf dist/*; rollup -c; ",
    "build-demo": "webpack --config ./webpack.config.js --mode production",
    "deploy": "npm run build-demo; touch docs/.nojekyll; gh-pages -d docs -t --git git",
    "lint": "eslint src tests rollup.config.js webpack.config.js",
    "precommit": "NODE_ENV=production lint-staged; npm run test",
    "prepublishOnly": "npm run test; rm -rf dist/*; npm run build;",
    "prerelease": "rm -rf dist/*; npm run build; zip -r dist.zip dist",
    "pretest": "npm run lint",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open",
    "test": "rollup -c ./rollup.test.config.js | tape-run --render='tap-spec'",
    "watch": "rollup -cw"
  },
  "dependencies": {
    "@flekschas/utils": "^0.28.0",
    "camera-2d-simple": "~2.2.1",
    "dom-2d-camera": "~1.2.3",
    "gl-matrix": "~3.3.0",
    "kdbush": "~3.0.0",
    "lodash-es": "~4.17.21",
    "pub-sub-es": "~2.0.1",
    "regl": "~2.1.0",
    "regl-line": "~0.4.4",
    "with-raf": "~1.1.1"
  },
  "peerDependencies": {
    "pub-sub-es": "~2.0.1",
    "regl": "~2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-regenerator": "^7.13.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "7.13.9",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "acorn": "^7.4.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "browser-env": "^3.3.0",
    "d3-axis": "^2.1.0",
    "d3-random": "^2.2.2",
    "d3-scale": "^3.3.0",
    "d3-selection": "^2.0.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "esm": "^3.2.25",
    "gh-pages": "^3.1.0",
    "gl": "^4.9.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "merge": "^2.1.1",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.48.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.2",
    "tap-spec": "^5.0.0",
    "tape-run": "^7.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-tape-run": "0.0.7",
    "zora": "^4.1.0"
  },
  "homepage": "https://github.com/flekschas/regl-scatterplot",
  "repository": {
    "type": "git",
    "url": "git://github.com/flekschas/regl-scatterplot.git"
  },
  "bugs": {
    "url": "https://github.com/flekschas/regl-scatterplot/issues"
  }
}
